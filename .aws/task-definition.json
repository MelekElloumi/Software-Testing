{
    "family": "melek-tsk-def-app4test-pipeline",
    "containerDefinitions": [
        {
            "name": "app4test-app",
            "image": "melekelloumi/app4test",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512"
}